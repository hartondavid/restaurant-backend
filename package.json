  "dependencies": {
    "@vercel/blob": "^1.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "multer": "^2.0.1",
    "node-fetch": "^3.3.2",
    "node-forge": "^1.3.1",
    "nodemon": "^3.1.10",
    "pg": "^8.16.3",
    "pg-types": "^4.0.2",
    "serverless-http": "^3.2.0",
    "update": "^0.7.4"
