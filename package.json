{
  "name": "restaurant-backend",
  "version": "1.0.0",
  "description": "To make it easy for you to get started with GitLab, here's a list of recommended next steps.",
  "main": "index.js",
  "scripts": {
    "start": "node server.mjs",
    "dev": "nodemon server.mjs",
    "build": "echo 'Backend build completed - no compilation needed'",
    "vercel-build": "npm run build",
    "migrate": "knex migrate:latest --knexfile knexfile.cjs",
    "seed": "knex seed:run --knexfile knexfile.cjs",
    "test-db": "node test-db-config.js",
    "setup-env": "node setup-env.js",
    "deploy": "npm start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/davidharton/restaurant-backend.git"
  },
